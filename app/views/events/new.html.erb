<h1>Creer un Evenement</h1>

 <%= form_tag url_for(action:'create'), method: "post" do %>

      <%= label_tag 'Price' %><br>
      <%= number_field_tag 'username' %>
      <br /> <br />

      <%= label_tag 'Title' %><br>
      <%= text_field_tag 'title'  %>
      <br /> <br />

      <%= label_tag 'Content' %><br>
      <%= text_area_tag 'content' %>
      <br /> <br />

      <%= submit_tag "Create Post" %>

    <% end %>
